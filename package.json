{
  "name": "gitty-probot-test",
  "description": "Probot Gitty Test built with Gatsby",
  "version": "1.0.0",
  "author": "Nnenna John <hello@nnennajohn.com",
  "keywords": [
    "probot",
    "gitty",
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "scripts": {
    "start": "gatsby develop --open",
    "build": "gatsby build",
    "clean": "rimraf ./public",
    "develop": "gatsby develop",
    "deploy": "gatsby build && gh-pages -d public",
    "format": "prettier -l --config ./.prettierrc  --ignore-path ./.prettierignore --write 'src/**/*.js'",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:scss": "stylelint --syntax=scss ./src/**/*.scss",
    "lint:js": "eslint . && flow check .",
    "lint:ci": "eslint . --cache --ext js,md --ignore-path .gitignore",
    "typecheck:ci": "flow check --include-warnings",
    "precommit": "lint-staged",
    "preview": "yarn clean && yarn build && http-server ./public -p 8000 -P http://localhost:8000",
    "test": "jest",
    "test:watch": "jest --watch",
    "test-coverage": "jest --coverage",
    "graphql": "gql-gen --url http://localhost:8000/___graphql --template typescript --out ./graphql-types.d.ts"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint",
      "flow focus-check",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "stylelint",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint --syntax=scss",
      "git add"
    ],
    "*.{md}": [
      "prettier --write",
      "git add",
      "git add"
    ]
  },
  "dependencies": {
    "gatsby": "^1.9.247",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-sass": "^1.0.26",
    "gatsby-plugin-sitemap": "^1.2.25",
    "gatsby-plugin-styled-components": "^2.0.11",
    "gatsby-source-filesystem": "^1.5.39",
    "gatsby-transformer-remark": "^1.7.44",
    "gh-pages": "^1.2.0",
    "jest-styled-components": "^5.0.1",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-github-button": "^0.1.11",
    "react-google-maps": "^9.4.5",
    "react-helmet": "^5.2.0",
    "react-popper": "^0.7.2",
    "styled-components": "^3.3.3",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.0.0-beta.51",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "flow-bin": "^0.75.0",
    "flow-typed": "^2.4.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.1.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.5",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2",
    "stylelint": "^9.3.0",
    "stylelint-config-prettier": "^3.3.0",
    "stylelint-config-primer": "^2.2.8",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "react",
          "es2015",
          "stage-1",
          "flow"
        ],
        "plugins": [
          "add-module-exports",
          "syntax-class-properties",
          "transform-class-properties"
        ]
      }
    }
  },
  "jest": {
    "setupFiles": [
      "./test/jestSetup.js"
    ],
    "setupTestFrameworkScriptFile": "./test/jestFrameworkSetup.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(jpg|png)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}"
    ],
    "moduleFileExtensions": [
      "jsx",
      "js"
    ],
    "transform": {
      ".(js|jsx)": "babel-jest"
    },
    "testRegex": "(\\.(test|spec))\\.(jsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.cache/"
    ],
    "modulePaths": [
      "src"
    ]
  }
}
